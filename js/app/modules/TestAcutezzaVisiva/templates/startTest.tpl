	<div id="header">
		<h1>Start Test</h1>
	</div>
	
	<div id="form>
		
		<div class="row">
			<span class="label">Username: </span>
			<input type="text" id="user.username" class="persistent" />
		</div>
		
		<div class="row">
			<span class="label">City: </span>
			<select id="user.city" class="persistent">
				<option value="Torino">Torino</option>
				<option value="Milano">Milano</option>
				<option value="Bologna">Bologna</option>
			</select>
		</div>
		
		<div class="row">
			<span class="label">Birth date: </span>
			<input type="date" id="user.birthdate" class="persistent" />
		</div>
		
		<div class="row">
			<span class="label">Favourite color: </span>
			<input type="color" id="user.color" class="persistent" />
		</div>
		
		
	</div>
	
	<div id="divPulsanti">
		<input id="buttonAvanti" class="action" type="button" name="next" value="avanti"/>
	</div>
